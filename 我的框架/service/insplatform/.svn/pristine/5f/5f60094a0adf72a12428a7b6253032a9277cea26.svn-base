package com.insplatform.module.course.coursekpointresource.model;

import java.io.Serializable;


import com.insframework.common.persistence.annotation.Table;

@Table("PX_COURSE_KPOINT_RESOURCE")
public class CourseKpointResource implements Serializable {
	/**
	 *
	 */
	private static final long serialVersionUID = 1L;
	
	private String resourceId;
	private double sortNumber;
	private String courseKpointId;

	public void setResourceId(String value) {
		this.resourceId = value;
	}
	
	public String getResourceId() {
		return this.resourceId;
	}
	
	public void setSortNumber(double value) {
		this.sortNumber = value;
	}
	
	public double getSortNumber() {
		return this.sortNumber;
	}
	
	public void setCourseKpointId(String value) {
		this.courseKpointId = value;
	}
	
	public String getCourseKpointId() {
		return this.courseKpointId;
	}
	
}
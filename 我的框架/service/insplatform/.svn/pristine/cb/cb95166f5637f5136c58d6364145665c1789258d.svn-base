package com.insplatform.module.bbs.article.model;

import java.io.Serializable;


import com.insframework.common.persistence.annotation.Id;
import com.insframework.common.persistence.annotation.Table;
import com.insframework.common.persistence.constant.SQL_CONSTANT.ID_STRATEGY;

@Table("PX_BBS_ARTICLE")
public class BbsArticle implements Serializable {
	/**
	 *
	 */
	private static final long serialVersionUID = 1L;
	
	@Id(strategy=ID_STRATEGY.UUID)
	private String id;
	private String articleTitle;
	private String createPersonId;
	private String createPersonName;
	private java.util.Date createTime;
	private String articleClassify;
	private double lookNum;
	private String ispublish;
	private String articleContent;
	private String acticlePhoto;

	public void setId(String value) {
		this.id = value;
	}
	
	public String getId() {
		return this.id;
	}
	
	public void setArticleTitle(String value) {
		this.articleTitle = value;
	}
	
	public String getArticleTitle() {
		return this.articleTitle;
	}
	
	public void setCreatePersonId(String value) {
		this.createPersonId = value;
	}
	
	public String getCreatePersonId() {
		return this.createPersonId;
	}
	
	public void setCreatePersonName(String value) {
		this.createPersonName = value;
	}
	
	public String getCreatePersonName() {
		return this.createPersonName;
	}
	
	public void setCreateTime(java.util.Date value) {
		this.createTime = value;
	}
	
	public java.util.Date getCreateTime() {
		return this.createTime;
	}
	
	public void setArticleClassify(String value) {
		this.articleClassify = value;
	}
	
	public String getArticleClassify() {
		return this.articleClassify;
	}
	
	public void setLookNum(double value) {
		this.lookNum = value;
	}
	
	public double getLookNum() {
		return this.lookNum;
	}
	
	public void setIspublish(String value) {
		this.ispublish = value;
	}
	
	public String getIspublish() {
		return this.ispublish;
	}
	
	public void setArticleContent(String value) {
		this.articleContent = value;
	}
	
	public String getArticleContent() {
		return this.articleContent;
	}
	
	public void setActiclePhoto(String value) {
		this.acticlePhoto = value;
	}
	
	public String getActiclePhoto() {
		return this.acticlePhoto;
	}
	
}
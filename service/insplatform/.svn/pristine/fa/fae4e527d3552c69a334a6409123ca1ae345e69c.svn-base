package com.insplatform.module.testpaper.service.impl;

import java.io.Serializable;
import java.util.*;


import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.insframework.system.base.repository.BaseRepository;
import com.insframework.system.base.service.impl.BaseServiceImpl;
import com.insframework.system.map.Condition;

import com.insplatform.module.testpaper.model.TestPaper;
import com.insplatform.module.testpaper.repository.TestPaperRepository;
import com.insplatform.module.testpaper.service.TestPaperService;

@Service
public class TestPaperServiceImpl extends BaseServiceImpl<TestPaper> implements TestPaperService{
	
	@Autowired
	private TestPaperRepository testPaperRepository;

	@Override
	public BaseRepository<TestPaper> getRepository() {		
		return testPaperRepository;
	}
	
	@Override
	public Map<String, Object> loadAllGrid(Condition condition) {			
		return testPaperRepository.loadAllGrid(condition);
	}
	
	@Override
	public List<Map<String, Object>> loadAllList(Condition condition) {			
		return testPaperRepository.loadAllList(condition);
	}
	
	@Override
	public Map<String, Object> load(String id) {			
		return testPaperRepository.load(id);
	}
	
	/**
	 * 重写父类get方法
	 */
	@Override
	public TestPaper get(Serializable id) {		
		return testPaperRepository.get(id);
	}
	
	/**
	 * 重写父类save方法
	 */
	@Override
	public Serializable save(TestPaper entity) {	
		return testPaperRepository.save(entity);
	}
	
	/**
	 * 重写父类update方法
	 */
	@Override
	public void update(TestPaper entity) {		
		testPaperRepository.update(entity);
	}
	
	/**
	 * 重写父类deleteById方法
	 */
	@Override
	public void deleteById(Serializable id) {		
		testPaperRepository.deleteById(id);
	}
	
	/**
	 * 重写父类deleteByIds方法
	 */
	@Override
	public void deleteByIds(String... ids) {		
		testPaperRepository.deleteByIds(ids);
	}


}

package com.insplatform.module.bbs.articlecommenthf.model;

import java.io.Serializable;


import com.insframework.common.persistence.annotation.Id;
import com.insframework.common.persistence.annotation.Table;
import com.insframework.common.persistence.constant.SQL_CONSTANT.ID_STRATEGY;

@Table("PX_BBS_ARTICLE_COMMENT_HF")
public class BbsArticleCommentHf implements Serializable {
	/**
	 *
	 */
	private static final long serialVersionUID = 1L;
	
	@Id(strategy=ID_STRATEGY.UUID)
	private String id;
	private String articleCommentId;
	private String content;
	private String createPersonId;
	private java.util.Date createTime;
	private String receiverPersonId;
	private String createPersonName;
	private String receiverPersonName;

	public void setId(String value) {
		this.id = value;
	}
	
	public String getId() {
		return this.id;
	}
	
	public void setArticleCommentId(String value) {
		this.articleCommentId = value;
	}
	
	public String getArticleCommentId() {
		return this.articleCommentId;
	}
	
	public void setContent(String value) {
		this.content = value;
	}
	
	public String getContent() {
		return this.content;
	}
	
	public void setCreatePersonId(String value) {
		this.createPersonId = value;
	}
	
	public String getCreatePersonId() {
		return this.createPersonId;
	}
	
	public void setCreateTime(java.util.Date value) {
		this.createTime = value;
	}
	
	public java.util.Date getCreateTime() {
		return this.createTime;
	}
	
	public void setReceiverPersonId(String value) {
		this.receiverPersonId = value;
	}
	
	public String getReceiverPersonId() {
		return this.receiverPersonId;
	}
	
	public void setCreatePersonName(String value) {
		this.createPersonName = value;
	}
	
	public String getCreatePersonName() {
		return this.createPersonName;
	}
	
	public void setReceiverPersonName(String value) {
		this.receiverPersonName = value;
	}
	
	public String getReceiverPersonName() {
		return this.receiverPersonName;
	}
	
}
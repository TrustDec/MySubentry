package com.insplatform.system.role;

/**
 * 角色常量
 * @author guom
 *
 */
public enum RoleConstant {
	
	/**
	 * 对应数据库中的超级管理员的id
	 */
	SYS_ADMIN_ID("sysadmin"),
	/**
	 * 对应数据库中的机构管理员的id
	 */
	ORG_ADMIN_ID("orgadmin"),
	/**
	 * 对应数据库中的默认角色
	 */
	DEFAULT("default");
	
	private final String value;	
	
	RoleConstant(String value) {
        this.value = value;
    }
    
    public String getValue() {
        return value;
    }	

}

<h1>DisplayField</h1> 
<h3>介绍</h3>
<p>
显示框
</p>
<img src="../../img/DisplayField.jpg" />
<h3>示例代码</h3>
<pre class="brush:js;">
Ext.define('App.demo.form.field.DisplayField',{
	
	init: function(){
		var me = this;
		var view = Ext.create('Ins.form.Form',{
			fields : [{
					//label
					text:"显示框",
					name:"Home",
					xtype:"ins_displayfield",
					value:'显示内容'
//					,
					//标签显示位置默认top,可选left,right
//					labelAlign:"left"
			}]
		});
		return view;
	}
});
</pre>
<h3>类名</h3>
<code>Ins.form.field.DisplayField</code>
<h4>别名</h4>
<code>ins_displayfield</code>
<h3>继承自</h3>
<code>Ext.form.field.Display</code>
<h3>构造对象</h3>
<pre class="brush:js;">
{
	text:"显示框",
	name:"name",
	xtype:"ins_displayfield",
	value:'显示内容'
}
</pre>
